<Window x:Class="BS92IB_HFT_2021222.WpfClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BS92IB_HFT_2021222.WpfClient"
        mc:Ignorable="d"
        Background="Aquamarine"
        Title="ShipUI" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.5*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Label>Welcome to the Fleet Manager!</Label>
        <StackPanel Orientation="Vertical" VerticalAlignment="Center" HorizontalAlignment="Stretch" Grid.Row="1">
            <Button Content="Fleets" Margin="10" Padding="10" Click="Button_click"></Button>
            <Button Content="Ships" Margin="10" Padding="10" Click="Button_click_1"></Button>
            <Button Content="Armaments" Margin="10" Padding="10" Click="Button_click_2"></Button>
            <Button Content="Weapons" Margin="10" Padding="10" Click="Button_click_3"></Button>

        </StackPanel>
    </Grid>
</Window>
